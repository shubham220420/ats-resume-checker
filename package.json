{
  "name": "ats-resume-checker",
  "version": "1.0.0",
  "description": "AI-powered ATS Resume Score Checker with semantic matching and improvement suggestions",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start"
  },
  "keywords": [
    "ats",
    "resume",
    "ai",
    "nlp",
    "job-matching",
    "recruitment"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "supertest": "^7.1.4"
  }
}
